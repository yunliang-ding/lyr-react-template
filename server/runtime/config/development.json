{
  "port": 8360,
  "workers": 1,
  "stickyCluster": true,
  "startServerTimeout": 3000,
  "reloadSignal": "SIGUSR2",
  "processKillTimeout": 10000,
  "jsonpCallbackField": "callback",
  "jsonContentType": "application/json",
  "jsonpContentType": "application/javascript",
  "errnoField": "errno",
  "errmsgField": "errmsg",
  "defaultErrno": 1000,
  "validateDefaultErrno": 1001,
  "env": "development",
  "cache": {
    "type": "file",
    "file": {
      "timeout": 86400000,
      "cachePath": "/home/lighthouse/local/thinkjs-icejs-template/server/runtime/cache",
      "pathDepth": 1,
      "gcInterval": 86400000
    }
  },
  "model": {
    "type": "mysql",
    "mysql": {
      "logConnect": true,
      "logSql": true,
      "database": "",
      "prefix": "think_",
      "encoding": "utf8",
      "host": "127.0.0.1",
      "port": "",
      "user": "root",
      "password": "root",
      "dateStrings": true
    }
  },
  "session": {
    "type": "file",
    "file": {
      "cookie": {
        "name": "thinkjs"
      },
      "sessionPath": "/home/lighthouse/local/thinkjs-icejs-template/server/runtime/session"
    }
  },
  "view": {
    "type": "nunjucks",
    "nunjucks": {
      "viewPath": "/home/lighthouse/local/thinkjs-icejs-template/server/www",
      "sep": "/",
      "extname": ".html"
    }
  },
  "logger": {
    "type": "console",
    "console": {},
    "file": {
      "backups": 10,
      "absolute": true,
      "maxLogSize": 51200,
      "filename": "/home/lighthouse/local/thinkjs-icejs-template/server/logs/app.log"
    },
    "dateFile": {
      "level": "ALL",
      "absolute": true,
      "pattern": "-yyyy-MM-dd",
      "alwaysIncludePattern": true,
      "filename": "/home/lighthouse/local/thinkjs-icejs-template/server/logs/app.log"
    }
  },
  "websocket": {
    "type": "socketio",
    "socketio": {
      "path": "/socket.io",
      "adapter": null,
      "messages": {
        "open": "/websocket/open",
        "addUser": "/websocket/addUser"
      }
    }
  }
}